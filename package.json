{
  "name": "overtype.com",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "d3": "^7.9.0",
    "npm": "^11.5.2",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^7.9.1",
    "react-router-dom": "^7.9.1",
    "yaml": "^2.8.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "xo && react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.28.5",
    "babel-jest": "^30.2.0",
    "eslint-config-xo": "^0.49.0",
    "eslint-config-xo-react": "^0.29.0",
    "jest": "^30.0.5",
    "react-scripts": "^5.0.1",
    "react-test-renderer": "^18.2.0",
    "xo": "^1.2.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "/node_modules/(?!d3|d3-array|internmap|delaunator|robust-predicates)"
    ]
  },
  "xo": {
    "prettier": true,
    "react": true,
    "rules": {
      "unicorn/filename-case": [
        "error",
        {
          "cases": {
            "camelCase": true,
            "pascalCase": true
          }
        }
      ]
    }
  }
}
